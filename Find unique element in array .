// hey , there we can find unique lement by sorting , unordered map , xor etc . 
//in this we are going to find unique element using xor

#include<iostream>
using namespace std;
int main(){
    int arr[]={2,5,6,3,3,6,2};
    int n = 7;
    int ans =0;
    for(int i = 0; i <n ; i++){
        ans = ans^ arr[i];    
    }
    cout<<"final " << ans;
}
// using xor u can only find single uique element . when using xor compiler convert no. into binary .
//so 0 ^ 2 = 2 , 2 ^ 5 = 7 , 7 ^ 6 =1 . so on  
