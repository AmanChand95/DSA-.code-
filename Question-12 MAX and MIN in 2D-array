#include<iostream>
#include<limits.h>
using namespace std;

int main(){
   int arr[2][3]={{1,2,3},
                  {4,5,6}};
   int max = INT_MIN;
   int min = INT_MAX;
   for(int i =0 ; i < 2; i++){
    for(int j = 0; j<3; j++){
      if(arr[i][j]>max){
         max=arr[i][j];
      }
      if(arr[i][j]<min){
         min = arr[i][j];
      }
     }
   }
   cout<<"minimum no of array"<<min;
   cout<<endl;
   cout<<"maximum no of array"<<max;
}
