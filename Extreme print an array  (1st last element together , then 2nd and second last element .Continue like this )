#include<iostream>
using namespace std;
int main(){ 
 int arr[]={1,2,3,4,5};
 int n = 5;
    for (int left = 0, right = n - 1; left <= right; left++, right--) {
        if (left == right) {
            cout << arr[left];
        } else {
            cout << arr[left] << " " << arr[right] << " ";
        }
    }
}
